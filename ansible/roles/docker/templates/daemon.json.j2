{
  "log-level": "warn",
  "log-driver": "json-file",
  "log-opts": {
    "max-size": "10m",
    "max-file": "5"
  }
{% if docker_insecure_registries|length > 0 %}
  ,
  "insecure-registries": [
  {% for reg in docker_insecure_registries %}
      "{{ reg }}"{% if not loop.last %},{% endif %}
  {% endfor %}

  ]
{% endif %}
}
